#include <iostream>

using namespace std;

int main() {
    const int rows = 10;
    const int cols = 10;
    int tablica[rows][cols] = {0};

    for (int i = 0; i < rows; i++) {
        tablica[i][0] = i; 
        tablica[i][1] = i + i; 
    }

    cout << "Tablica 10x10:" << endl;
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < 2; j++) {
            cout << tablica[i][j] << "\t";
        }
        cout << endl;
    }

    return 0;
}
