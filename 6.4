#include <iostream>
#include <string>
using namespace std;

int main() {
    string tekst;
    int n;

    cout << "Podaj string: ";
    cin >> tekst;

    cout << "Podaj liczbe n: ";
    cin >> n;

    string wynik;

    for (int i = 0; i < n; i++) {
        wynik += tekst;
    }

    cout << "Wynik konkatenacji: " << wynik << endl;

    return 0;
}
