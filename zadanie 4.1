#include <iostream>
using namespace std;

int main() {
int n;
cout << "Podaj liczbe elementow: ";
cin >> n;

int tablica[n];
cout << "Podaj " << n << " liczb calkowitych: " << endl;

for (int i = 0; i < n; i++) {
cin >> tablica[i];
}

int MaxElement = tablica[0];
for (int i = 1; i < n; i++) {
if (tablica[i] > MaxElement) {
MaxElement = tablica[i];
}
}

cout << "Najwiekszy element w tablicy to: " << MaxElement << endl;

return 0;
}
