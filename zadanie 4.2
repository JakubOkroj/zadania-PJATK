#include <iostream>
using namespace std;

int main() {
int n;
cout << "Podaj liczbe elementow: ";
cin >> n;

int tablica[n];
cout << "Podaj " << n << " liczb calkowitych: " << endl;

for (int i = 0; i < n; i++) {
cin >> tablica[i];
}

int najczestszy = tablica[0], MaxWystepien = 0;

for (int i = 0; i < n; i++) {
int licznik = 0;
for (int j = 0; j < n; j++) {
if (tablica[i] == tablica[j]) {
licznik++;
}
}
if (licznik > MaxWystepien) {
MaxWystepien = licznik;
najczestszy = tablica[i];

}
}
cout << "Najczesciej wystepujacy element to: " << najczestszy << ", liczba wystapien: " << MaxWystepien << endl;


return 0;
}
