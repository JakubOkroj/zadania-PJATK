#include <iostream>
#include <vector>

using namespace std;

int main() {
    int rows, numColumns;

    cout << "Podaj liczbe wierszy: ";
    cin >> rows;

    cout << "Podaj liczbe kolumn: ";
    cin >> numColumns;

    vector<vector<int>> tablica(rows, vector<int>(numColumns));

    cout << "Podaj elementy tablicy:" << endl;
    for (int i = 0; i < rows; ++i) {
        for (int j = 0; j < numColumns; ++j) {
            cin >> tablica[i][j];
        }
    }

    cout << "Tablica wejsciowa:" << endl;
    for (int i = 0; i < rows; ++i) {
        for (int j = 0; j < numColumns; ++j) {
            cout << tablica[i][j] << " ";
        }
        cout << endl;
    }

    for (int i = 0; i < rows; ++i) {
        for (int j = 0; j < numColumns / 2; ++j) {
            swap(tablica[i][j], tablica[i][numColumns - j - 1]);
        }
    }

    cout << "Tablica wyjsciowa:" << endl;
    for (int i = 0; i < rows; ++i) {
        for (int j = 0; j < numColumns; ++j) {
            cout << tablica[i][j] << " ";
        }
        cout << endl;
    }

    return 0;
}
